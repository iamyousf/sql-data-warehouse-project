USE Datawarehouse;
--=================================================================================================
---For Bronze.crm_cust_info
--=================================================================================================
SELECT * FROM bronze.crm_cust_info;
--CHECKING FOR NULL OR DULIPCATES IN PRIMARY KEY 
SELECT 
    cst_id,
    COUNT(*) AS cnt
FROM bronze.crm_cust_info
GROUP BY cst_id
HAVING COUNT(*) > 1 OR cst_id IS NULL;
--CHECKING THE NULL OR DULICATES 
SELECT * FROM bronze.crm_cust_info
WHERE cst_id=29466;
--QUERY FOR THAT 
SELECT 
    cst_id,
    COUNT(*) AS cnt
FROM bronze.crm_cust_info
GROUP BY cst_id
HAVING COUNT(*) > 1 OR cst_id IS NULL;

SELECT * FROM bronze.crm_cust_info
WHERE cst_id=29466;
--CHECKING UNWANTED SPACE IN FRIST LETTER
SELECT cst_firstname
FROM bronze.crm_cust_info
WHERE cst_firstname <> TRIM(cst_firstname);
--CHECKING UNWANTED SPACE IN FRIST LETTER
SELECT cst_lastname
FROM bronze.crm_cust_info
WHERE cst_lastname <> TRIM(cst_lastname)
--CHECKING DISTICT IN GNDR & MATERIAL_STATUS 
SELECT DISTINCT cst_gndr
FROM bronze.crm_cust_info
SELECT DISTINCT cst_material_status
FROM bronze.crm_cust_info
--DELETE NULL CST_ID
DELETE FROM bronze.crm_cust_info
WHERE cst_id IS NULL;

--=================================================================================================
---For Silver.crm_cust_info
--=================================================================================================
SELECT * FROM silver.crm_cust_info;
--CHECKING FOR NULL OR DUPLICATES IN PRIMARY KEY 
SELECT 
    cst_id,
    COUNT(*) AS cnt
FROM silver.crm_cust_info
GROUP BY cst_id
HAVING COUNT(*) > 1 OR cst_id IS NULL;

--CHECKING THE NULL OR DUPLICATES 
SELECT * FROM silver.crm_cust_info
WHERE cst_id = 29466;

--QUERY FOR THAT 
SELECT 
    cst_id,
    COUNT(*) AS cnt
FROM silver.crm_cust_info
GROUP BY cst_id
HAVING COUNT(*) > 1 OR cst_id IS NULL;

SELECT * FROM silver.crm_cust_info
WHERE cst_id = 29466;

--CHECKING UNWANTED SPACE IN FIRST LETTER
SELECT cst_firstname
FROM silver.crm_cust_info
WHERE cst_firstname <> TRIM(cst_firstname);

--CHECKING UNWANTED SPACE IN LAST LETTER
SELECT cst_lastname
FROM silver.crm_cust_info
WHERE cst_lastname <> TRIM(cst_lastname);

--CHECKING DISTINCT IN GNDR & MATERIAL_STATUS 
SELECT DISTINCT cst_gndr
FROM silver.crm_cust_info;

SELECT DISTINCT cst_material_status
FROM silver.crm_cust_info;

--DELETE NULL CST_ID
DELETE FROM silver.crm_cust_info
WHERE cst_id IS NULL;

--=================================================================================================
---For Bronze.crm_prd_info
--=================================================================================================
SELECT * FROM bronze.crm_prd_info;

--CHECKING NULL OR DULIPCATES IN PRIMARY KEY 
SELECT 
prd_id,
COUNT(*) AS BANKIA
FROM bronze.crm_prd_info
GROUP BY prd_id
HAVING COUNT(*) >1 OR prd_id IS NULl;

--CHECKING FOR EXTRA SPACE IN STRINGS
SELECT prd_nm
FROM bronze.crm_prd_info
WHERE prd_nm <> TRIM(prd_nm);

--CHECKING NAGATIVE OR NULL VALUE
SELECT prd_cost
FROM  bronze.crm_prd_info
WHERE prd_cost < 0 or prd_cost IS NULL;

--CHECKING DISTINT FOR PRD LINE
SELECT DISTINCT prd_line
FROM bronze.crm_prd_info;

--CHECKING FOR INVALID DATES 
SELECT * FROM bronze.crm_prd_info
WHERE prd_end_dt< prd_start_dt;

--=================================================================================================
---For Silver.crm_prd_info
--=================================================================================================
SELECT * FROM silver.crm_prd_info;

--CHECKING NULL OR DUPLICATES IN PRIMARY KEY 
SELECT 
    prd_id,
    COUNT(*) AS BANKIA
FROM silver.crm_prd_info
GROUP BY prd_id
HAVING COUNT(*) > 1 OR prd_id IS NULL;

--CHECKING FOR EXTRA SPACE IN STRINGS
SELECT prd_nm
FROM silver.crm_prd_info
WHERE prd_nm <> TRIM(prd_nm);

--CHECKING NEGATIVE OR NULL VALUE
SELECT prd_cost
FROM silver.crm_prd_info
WHERE prd_cost < 0 OR prd_cost IS NULL;

--CHECKING DISTINCT FOR PRD LINE
SELECT DISTINCT prd_line
FROM silver.crm_prd_info;

--CHECKING FOR INVALID DATES 
SELECT *
FROM silver.crm_prd_info
WHERE prd_end_dt < prd_start_dt;

--=================================================================================================
---For Bronze.crm_sales_details
--=================================================================================================
--CHECKING FOR EXTRA SPACES IN SLS_ORDER_NUM
SELECT * FROM bronze.crm_sales_details
WHERE sls_ord_num <> TRIM(sls_ord_num);

--CHECKING FOR PRD KEY IS SLIVER TABLES 
SELECT * FROM bronze.crm_sales_details
WHERE sls_prd_key NOT IN (SELECT prd_key FROM silver.crm_prd_info)

--CHECKING FOR CRD KEY IS SLIVER TABLES 
SELECT * FROM bronze.crm_sales_details
WHERE sls_cust_id NOT IN (SELECT cst_id FROM silver.crm_cust_info);

--CHECKING FOR ZERO OR DATE !=8 AND COVERT
SELECT 
NULLIF(sls_order_dt,0) sls_order_dt
FROM bronze.crm_sales_details
WHERE sls_order_dt <=0
OR LEN(sls_order_dt) !=8
OR LEN (sls_order_dt) > 20500101
OR LEN (sls_order_dt) < 19900101;

--CHECKING FOR SALES ,QUANTITY ,PRICE
SELECT
sls_sales,
sls_quantity,
sls_price
FROM bronze.crm_sales_details
WHERE sls_sales != sls_quantity * sls_price
OR sls_sales IS NULL OR sls_quantity IS NULL OR sls_price IS NULL
OR sls_sales <=0 OR sls_quantity <=0 or sls_price <=0
ORDER BY sls_sales;

--=================================================================================================
---For Silver.crm_sales_details
--=================================================================================================

--CHECKING FOR EXTRA SPACES IN SLS_ORDER_NUM
SELECT * 
FROM silver.crm_sales_details
WHERE sls_ord_num <> TRIM(sls_ord_num);

--CHECKING FOR PRD KEY IS SILVER TABLES 
SELECT * 
FROM silver.crm_sales_details
WHERE sls_prd_key NOT IN (SELECT prd_key FROM silver.crm_prd_info);

--CHECKING FOR CST KEY IS SILVER TABLES 
SELECT * 
FROM silver.crm_sales_details
WHERE sls_cust_id NOT IN (SELECT cst_id FROM silver.crm_cust_info);

--CHECKING FOR ZERO OR DATE !=8 AND CONVERT
SELECT sls_order_dt
FROM silver.crm_sales_details
WHERE sls_order_dt IS NULL
   OR sls_order_dt > '2050-01-01'
   OR sls_order_dt < '1990-01-01';



--CHECKING FOR SALES ,QUANTITY ,PRICE
SELECT DISTINCT
    sls_sales,
    sls_quantity,
    sls_price
FROM silver.crm_sales_details
WHERE sls_sales <> sls_quantity * sls_price
   OR sls_sales IS NULL 
   OR sls_quantity IS NULL 
   OR sls_price IS NULL
   OR sls_sales <= 0 
   OR sls_quantity <= 0 
   OR sls_price <= 0
ORDER BY sls_sales;


--=================================================================================================
---For Bronze.erp_cust_az12
--=================================================================================================

--CHECKING AND SPLIT THE STRING 
SELECT
CASE
    WHEN cid LIKE 'NAS%' THEN SUBSTRING(cid,4,LEN(cid))
    ELSE cid
END AS cid
FROM bronze.erp_cust_az12;

--CHECKING FOR BRITHDATE OLD OR FUTURE
SELECT DISTINCT
bdate FROM bronze.erp_cust_az12
WHERE bdate <'1924-01-01' Or bdate > GETDATE();

--CHECKING FOR GEN 
SELECT DISTINCT
gen FROM bronze.erp_cust_az12;

--=================================================================================================
---For Silver.erp_cust_az12
--=================================================================================================

--CHECKING AND SPLIT THE STRING 
SELECT
CASE
    WHEN cid LIKE 'NAS%' THEN SUBSTRING(cid,4,LEN(cid))
    ELSE cid
END AS cid
FROM silver.erp_cust_az12;

--CHECKING FOR BRITHDATE OLD OR FUTURE
SELECT DISTINCT
bdate FROM silver.erp_cust_az12
WHERE bdate <'1924-01-01' Or bdate > GETDATE();

--CHECKING FOR GEN 
SELECT DISTINCT
gen FROM silver.erp_cust_az12;

--=================================================================================================
---For Bronze.erp_loc_a101
--=================================================================================================

--REMOVING - FROM CID AND REPLACE IT TO NULL OR NOTHING 
SELECT
REPLACE (cid,'-','') cid
FROM bronze.erp_loc_a101;

--CHECKING DISTINCT IN CNTRY AND FIX IT 
SELECT DISTINCT
cntry FROM bronze.erp_loc_a101
ORDER BY cntry

--=================================================================================================
---For Silver.erp_loc_a101
--=================================================================================================

--REMOVING - FROM CID AND REPLACE IT TO NULL OR NOTHING 
SELECT
REPLACE(cid,'-','') AS cid
FROM silver.erp_loc_a101;

--CHECKING DISTINCT IN CNTRY AND FIX IT 
SELECT DISTINCT
cntry
FROM silver.erp_loc_a101
ORDER BY cntry;

--=================================================================================================
--For Bronze.erp_px_cat_g1v2
--=================================================================================================
--CHECKING FOR ANY UNWANTED SPACES
SELECT * FROM bronze.erp_px_cat_g1v2
WHERE cat<> TRIM(cat) OR subcat<> TRIM(subcat) OR maintenance<> TRIM(maintenance) 


--DATA STANDRAIZATION AND CONSENCTENCY 
SELECT DISTINCT
cat FROM bronze.erp_px_cat_g1v2

SELECT DISTINCT
subcat FROM bronze.erp_px_cat_g1v2

SELECT DISTINCT
maintenance FROM bronze.erp_px_cat_g1v2

--=================================================================================================
--For Silver.erp_px_cat_g1v2
--=================================================================================================
--CHECKING FOR ANY UNWANTED SPACES
SELECT * FROM silver.erp_px_cat_g1v2
WHERE cat <> TRIM(cat) 
   OR subcat <> TRIM(subcat) 
   OR maintenance <> TRIM(maintenance);


--DATA STANDRAIZATION AND CONSENCTENCY 
SELECT DISTINCT
cat FROM silver.erp_px_cat_g1v2;

SELECT DISTINCT
subcat FROM silver.erp_px_cat_g1v2;

SELECT DISTINCT
maintenance FROM silver.erp_px_cat_g1v2;




